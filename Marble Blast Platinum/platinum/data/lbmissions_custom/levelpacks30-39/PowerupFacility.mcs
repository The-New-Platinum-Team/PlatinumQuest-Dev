//Put any scripts that will be loaded after the mission is loaded here

package specialHelicopter {

   function PowerupFacility_AddHelicopter() {
      commandToClient(LocalClientConnection, 'doPowerUp', HelicopterItem.powerUpId);
      HelicopterItem.onUse(0, MPGetMyMarble());
   }
   function PowerupFacility_KeepHelicopter() {
      PowerupFacility_AddHelicopter();
      $PowerupFacility_KeepHelicopter = schedule(90, 0, PowerupFacility_KeepHelicopter);
   }
   function PowerupFacility_CancelHelicopter() {
      cancel($PowerupFacility_KeepHelicopter);
      %player = localClientConnection.player;
      clientCmdPushTimer(HelicopterItem.powerUpId, 0, 0);
      onNextFrame(clientCmdPushTimer, HelicopterItem.powerUpId, 0, 0); // Without this, sometimes there's like a schedule race condition or something that doesn't cancel the timer

      %player.setPowerUpId(0, true); // This is part of the 'mouseFire' function, so the whole idea is that it sets the powerup slot to empty I guess.

      // Code copied from the mission reset code. The code already doesn't allow other powerups to be shared with the Gyrocopter.
      for (%i = 0; %i < 20; %i ++) {
         LocalClientConnection.deactivatePowerup(%i);
         LocalClientConnection.unmountPlayerImage(%i);
         cancel(LocalClientConnection.player.powerupSchedule[%i]);
         cancel(LocalClientConnection.unmount[%i]);
      }
   }

   function mouseFire(%val) {
      if ($BUZZ::foundHelicopter && %val)
      {
         // Jeff: if we have found the helicopter, set the powerup!
         $BUZZ::usingHelicopter = !$BUZZ::usingHelicopter;
         if ($BUZZ::usingHelicopter) {
            PowerupFacility_KeepHelicopter();
         } else {
            PowerupFacility_CancelHelicopter();
         }
      }
      Parent::mouseFire(%val);
   }

   function PowerupFacility_CannotPickUpOtherPowerupsTimeout () {
      $BUZZ::cannotPickUpOtherPowerupsMessage = false;
   }
 
   function PowerUp::onPickup(%this, %obj, %user, %amount) {
      %name = %this.getName();
   
      if (%name !$= "TimeTravelItem") {
         // Jeff: can't pick up more powerups if you have the helicopter.
         // wanna know why? Cuz Buzzmusic100 told me so.
         if ($BUZZ::foundHelicopter) {
            if ($BUZZ::cannotPickUpOtherPowerupsMessage == true || %this.getName() $= "HelicopterItem") {
               // Do not spam this message (it has a timeout), and don't get the powerup if it's the Gyrocopter again
               return;
            }
            messageClient(%user.client, '', '\c0You can not pick up any other PowerUps at this time!');
            $BUZZ::CannotPickUpOtherPowerupsTimeout = schedule(5000, 0, PowerupFacility_CannotPickUpOtherPowerupsTimeout);
            $BUZZ::cannotPickUpOtherPowerupsMessage = true;
            return;
         }
      
         // Jeff: if it is a helicopter, set the variable to true
         if (%this.getName() $= "HelicopterItem" && %obj.powerupfacility $= "1" && MissionInfo.powerupfacility $= "1") {
            $BUZZ::foundHelicopter = true;
         }
      }
 
      Parent::onPickup(%this, %obj, %user, %amount);
   }
 
   function clientCbOnMissionLoaded() {
      // Jeff: initalize important variables
      $BUZZ::usingHelicopter = false;
      $BUZZ::foundHelicopter = false;
      addMaterialMapping( "mbp_grid_orangebounce" , BuzzHighBounce);
      addMaterialMapping( "mbp_grid_pinkfake" , BuzzHighBounce);
      addMaterialMapping( "mbp_grid_bluerandom" , BuzzHighBounce);
      addMaterialMapping( "chaosrandom" , BuzzLowerBounce);
      PowerupFacility_CancelHelicopter();
   }
 
   function clientCbOnMissionReset() {
      // Jeff: reinitalize important variables
      $BUZZ::usingHelicopter = false;
      $BUZZ::foundHelicopter = false;
 
      PowerupFacility_CancelHelicopter();
   }

   function clientCbOnMissionEnded() {
      deactivatePackage(specialHelicopter);
      // These frictions shouldn't appear in other levels, but just in case:
      addMaterialMapping( "mbp_grid_orangebounce" , DefaultMaterial);
      addMaterialMapping( "mbp_grid_pinkfake" , DefaultMaterial);
      addMaterialMapping( "mbp_grid_bluerandom" , DefaultMaterial);
      addMaterialMapping( "chaosrandom" , DefaultMaterial);
   }
};

activatePackage(specialHelicopter);

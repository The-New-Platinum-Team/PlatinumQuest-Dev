new AudioProfile(BassPunchSfx) {
   fileName = "~/data/sound/bass_punch.wav";
   description = AudioLooping2D;
   preload = true;
};

function MissionStartup() {
	alxStop($bassHandle);

	schedule(10, 0, punchIt);
}
function clientCbOnMissionEnded() {
	alxStop($bassHandle);
}
function punchIt() {
	alxStop($bassHandle);
	$bassHandle = alxPlay(BassPunchSfx);
}
